{
  "name": "react-rubiks-cube",
  "version": "1.0.0",
  "description": "An interactive Rubik's cube in pure React/CSS3",
  "main": "index.js",
  "scripts": {
    "dev": "node server.js",
    "clean": "rm -rf dist assets",
    "build": "npm run clean && webpack && mkdir dist && cp -rvp assets index.html dist/",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jedwards1211/react-rubiks-cube"
  },
  "keywords": [
    "react",
    "rubik",
    "cube",
    "css3",
    "transforms"
  ],
  "author": "Andy Edwards",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jedwards1211/react-rubiks-cube/issues"
  },
  "homepage": "https://github.com/jedwards1211/react-rubiks-cube",
  "dependencies": {
    "lodash": "^3.10.0",
    "node-sass": "^3.2.0",
    "react": "^0.13.3",
    "react-vendor-prefix": "0.0.1",
    "sass-loader": "^1.0.3"
  },
  "devDependencies": {
    "babel": "^5.8.20",
    "babel-core": "^5.8.20",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.15.6",
    "gh-pages": "^2.0.1",
    "react-hot-loader": "^1.2.8",
    "style-loader": "^0.12.3",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1"
  }
